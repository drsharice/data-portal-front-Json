{
  "openapi": "3.0.3",
  "info": {
    "title": "Data Edge API \u2013 Finance Budget",
    "version": "0.1.0"
  },
  "paths": {
    "/sources": {
      "get": {
        "summary": "List Sources",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "sources": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/data/{source}": {
      "get": {
        "summary": "Get Data",
        "parameters": [
          {
            "name": "source",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "limit",
            "in": "query",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "additionalProperties": true
                  }
                }
              }
            }
          }
        }
      }
    },
    "/data/{source}/download": {
      "get": {
        "summary": "Download CSV",
        "responses": {
          "200": {
            "description": "CSV file"
          }
        }
      }
    }
  }
}